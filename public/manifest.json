{
    "manifest_version": 2,
    "name": "post-woman",
    "version": "1.0.0",
    "description": "post woman",
    "background": {
        "page": "background.html"
    },
    "browser_action": {
        "default_icon": "icon.png",
        "default_title": "这是一个示例Chrome插件",
        "default_popup": "popup.html"
    },
    "permissions": [
        "contextMenus",
        "tabs",
        "notifications",
        "webRequest",
        "webRequestBlocking",
        "storage",
        "http://*/*",
        "https://*/*"
    ],
    "homepage_url": "https://www.xiaobailong.top"
}